{
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "deploy": "npm version patch && npm publish"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "nextjs-progressloader",
  "author": "Samuel Matos",
  "module": "dist/nextjs-progressloader.esm.js",
  "size-limit": [
    {
      "path": "dist/nextjs-progressloader.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nextjs-progressloader.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "@types/nprogress": "^0.2.2",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "husky": "^8.0.3",
    "next": "^13.5.6",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "nprogress": "^0.2.0"
  }
}
